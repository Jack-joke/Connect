<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.inscripts">

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="DATABASE"
            android:value="inscripts_cc.db" />
        <meta-data
            android:name="VERSION"
            android:value="3" />
        <meta-data
            android:name="QUERY_LOG"
            android:value="false" />
        <meta-data
            android:name="DOMAIN_PACKAGE_NAME"
            android:value="com.inscripts.models" />

        <activity
            android:name=".activities.UrlInitializerActivity"
            android:theme="@style/materilbaseTheme" />
        <activity
            android:name=".activities.LoginActivity"
            android:theme="@style/materilbaseTheme" />
        <activity
            android:name=".activities.CometChatActivity"
            android:configChanges="orientation|screenSize"
            android:launchMode="singleTask"
            android:theme="@style/materilbaseTheme" />
        <activity
            android:name=".activities.ChatSettingsActivity"
            android:theme="@style/materilbaseTheme" />
        <activity
            android:name=".activities.UnblockuserActivity"
            android:theme="@style/materilbaseTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activities.CometChatActivity" />
        </activity>
        <activity
            android:name=".activities.SingleChatActivity"
            android:parentActivityName=".activities.CometChatActivity"
            android:theme="@style/materilbaseTheme"
            android:windowSoftInputMode="stateHidden">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activities.CometChatActivity" />
        </activity>
        <activity
            android:name=".activities.ImagePreviewActivity"
            android:theme="@style/Theme.FullscreeTransperent" />
        <activity
            android:name=".activities.ChatroomActivity"
            android:theme="@style/materilbaseTheme"
            android:windowSoftInputMode="stateHidden">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activities.CometChatActivity" />
        </activity>
        <activity
            android:name=".activities.CreateChatroomActivity"
            android:theme="@style/materilbaseTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activities.ChatroomActivity" />
        </activity>
        <activity
            android:name=".activities.ShowChatroomUsersActivity"
            android:theme="@style/materilbaseTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activities.ChatroomActivity" />
        </activity>
        <activity
            android:name=".activities.InviteUserActivity"
            android:theme="@style/materilbaseTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activities.ChatroomActivity" />
        </activity>
        <activity
            android:name=".videochat.IncomingCallActivity"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen" />
        <activity
            android:name=".videochat.OutgoingCallActivity"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen" />
        <activity android:name=".videochat.VideoChatActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activities.SingleChatActivity" />
        </activity>
        <activity android:name=".activities.InviteAVBroadcastUsers">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".videochat.VideoChatActivity" />
        </activity>
        <activity
            android:name=".activities.InviteViaSmsActivity"
            android:theme="@style/materilbaseTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activities.CometChatActivity" />
        </activity>
        <activity
            android:name=".activities.BlankActivity"
            android:label="@string/title_activity_blank"
            android:theme="@style/materilbaseTheme"></activity>
        <activity
            android:name=".activities.WhiteboardActivity"
            android:label="@string/title_activity_blank"
            android:theme="@style/materilbaseTheme"></activity>
        <activity
            android:name=".activities.WriteBoardActivity"
            android:label="@string/title_activity_blank"
            android:theme="@style/materilbaseTheme"></activity>
        <activity android:name=".activities.HandwriteActivity" />
        <activity
            android:name=".activities.ViewUserProfileActivity"
            android:parentActivityName=".activities.CometChatActivity"
            android:theme="@style/materilbaseTheme"
            android:windowSoftInputMode="stateHidden">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activities.SingleChatActivity" />
        </activity>
        <activity
            android:name=".activities.SettingActivity"
            android:parentActivityName=".activities.CometChatActivity"
            android:theme="@style/materilbaseTheme"
            android:windowSoftInputMode="stateHidden"></activity>
        <activity
            android:name=".activities.StatusMessageActivity"
            android:parentActivityName=".activities.SettingActivity"
            android:theme="@style/materilbaseTheme"
            android:windowSoftInputMode="stateHidden">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activities.SingleChatActivity" />
        </activity>
        <activity
            android:name=".activities.SelectLanguageActivity"
            android:parentActivityName=".activities.SettingActivity"
            android:theme="@style/materilbaseTheme"
            android:windowSoftInputMode="stateHidden">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activities.SingleChatActivity" />
        </activity>
        <activity
            android:name=".activities.SinglePlayerGameActivity"
            android:parentActivityName=".activities.SettingActivity"
            android:theme="@style/materilbaseTheme"
            android:windowSoftInputMode="stateHidden">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activities.SingleChatActivity" />
        </activity>
        <activity
            android:name=".activities.BroadcastMessageActivity"
            android:parentActivityName=".activities.CometChatActivity"
            android:theme="@style/materilbaseTheme"
            android:windowSoftInputMode="stateHidden">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activities.SingleChatActivity" />
        </activity>
        <activity
            android:name=".activities.BotDetailActivity"
            android:parentActivityName=".activities.BotsActivity"
            android:theme="@style/materilbaseTheme"
            android:windowSoftInputMode="stateHidden">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activities.SingleChatActivity" />
        </activity>
        <activity
            android:name=".activities.AnnouncementActivity"
            android:parentActivityName=".activities.SettingActivity"
            android:theme="@style/materilbaseTheme"
            android:windowSoftInputMode="stateHidden">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activities.SingleChatActivity" />
        </activity>
        <activity android:name=".activities.BotsActivity"
            android:parentActivityName=".activities.SettingActivity"
            android:theme="@style/materilbaseTheme"
            android:windowSoftInputMode="stateHidden"></activity>
    </application>

</manifest>